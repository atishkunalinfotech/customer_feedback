<p id="notice"><%= notice %></p>

<h1>Admin Nature Of Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th>Review type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_nature_of_reviews.each do |admin_nature_of_review| %>
      <tr>
        <td><%= admin_nature_of_review.title %></td>
        <td><%= admin_nature_of_review.user_id %></td>
        <td><%= admin_nature_of_review.review_type %></td>
        <td><%= link_to 'Show', admin_nature_of_review %></td>
        <td><%= link_to 'Edit', edit_admin_nature_of_review_path(admin_nature_of_review) %></td>
        <td><%= link_to 'Destroy', admin_nature_of_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Nature Of Review', new_admin_nature_of_review_path %>
